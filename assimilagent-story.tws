Vversion.extensions['gameMacros'] = {\u000a	major: 1,\u000a	minor: 0,\u000a	revision: 0\u000a};\u000amacros['animateStart'] = {\u000a	handler: function (g, e, f, b) {\u000a		macros['animateStart'].interval =\u000a		setInterval(function() {\u000a			var startTag = $(f[0]);\u000a			if (startTag != null) {\u000a				var currentColour = startTag.currentColour;\u000a				var nextColour;\u000a				if (currentColour == null || currentColour == "#ffffff") {\u000a					nextColour = "#888888";\u000a				} else if (currentColour == "#888888") {\u000a					nextColour = "#111111";\u000a				} else {\u000a					nextColour = "#ffffff";\u000a				}\u000a				startTag.style.color = nextColour;\u000a				startTag.currentColour = nextColour;\u000a			}\u000a		}, 40);\u000a	}\u000a}\u000amacros['stopStartAnimation'] = {\u000a	handler: function (g, e, f, b) {\u000a		if (macros['animateStart'].interval != null) {\u000a			clearInterval(macros['animateStart'].interval);\u000a			macros['animateStart'].interval = null;\u000a		}\u000a	}\u000a}\u000amacros['audioLoop'] = {\u000a	audioLoops: {},\u000a	init: function() {\u000a		HTMLAudioElement.prototype.playLoop = function() {\u000a			if (this.resetTo == 0) {\u000a				// Easy - just create a normal interval as we've already done\u000a				this.play();\u000a\u000a				// Have to save "this" for the anon function\u000a				var _self = this;\u000a				this.interval = setInterval(function() {\u000a					_self.currentTime = _self.resetTo;\u000a				}, this.loopLen);\u000a			} else {\u000a				// This is the "hard" case because the loops are two different lengths - start by saving the tag\u000a				var _self = this;\u000a				var repeatLen = this.loopLen - (1000 * this.resetTo);\u000a\u000a				this.play();\u000a				setTimeout(function() {\u000a					_self.currentTime = _self.resetTo;\u000a					_self.interval = setInterval(function() {\u000a						_self.currentTime = _self.resetTo;\u000a					}, repeatLen);\u000a				}, this.loopLen);\u000a			}\u000a		}\u000a		HTMLAudioElement.prototype.stopLoop = function() {\u000a			clearInterval(this.interval);\u000a			this.pause();\u000a			this.currentTime = 0;\u000a		}\u000a	},\u000a	handler: function (g, e, f, b) {\u000a		var audioTag = f[0];\u000a		var audioSource = f[1];\u000a		var loopLen = f[2];\u000a		var restartAt = (f[3] > 0) ? f[3] : 0;\u000a\u000a		var tag = document.createElement("audio");\u000a		var source= document.createElement('source');\u000a		if (tag.canPlayType('audio/mpeg;')) {\u000a		    source.type= 'audio/mpeg';\u000a		    source.src= audioSource + '.mp3';\u000a		} else {\u000a		    source.type= 'audio/ogg';\u000a		    source.src= audioSource + '.ogg';\u000a		}\u000a		tag.appendChild(source);\u000a		tag.load();\u000a\u000a		// Save the loop info\u000a		tag.audioTag = audioTag;\u000a		tag.loopLen = loopLen;\u000a		tag.resetTo = restartAt;\u000a\u000a		// And whether we can play or not\u000a		tag.isReady = false;\u000a\u000a		// Save the tag\u000a		macros['audioLoop'].audioLoops[audioTag] = tag;\u000a\u000a		tag.listener = function() {\u000a			tag.isReady = true;\u000a			tag.removeEventListener('canplaythrough', this.listener);\u000a		}\u000a		tag.addEventListener('canplaythrough', tag.listener, true);\u000a	}\u000a}\u000amacros['playAudio'] = {\u000a	pendingIntervals: {},\u000a	handler: function (g, e, f, b) {\u000a		var audioTag = f[0];\u000a		var tag = macros['audioLoop'].audioLoops[audioTag];\u000a		if (tag == null) {\u000a			console.log("Tried to play audio stream tagged \u005c'" + audioTag + "\u005c' without loading it.");\u000a			return;\u000a		}\u000a\u000a		// Can we just play?\u000a		if (tag.isReady) {\u000a			tag.play();\u000a		} else {\u000a			// The audio wasn't loaded yet - we could do this a better way, but just to avoid a race condition\u000a			// we're just going to poll\u000a			macros['playAudio'].pendingIntervals[audioTag] = setInterval(function() {\u000a				if (tag.isReady) {\u000a					tag.play();\u000a					clearInterval(macros['playAudio'].pendingIntervals[audioTag]);\u000a					macros['playAudio'].pendingIntervals[audioTag] = null;\u000a				}\u000a			}, 10);\u000a		}\u000a	}\u000a}\u000amacros['startAudioLoop'] = {\u000a	pendingIntervals: {},\u000a	handler: function (g, e, f, b) {\u000a		var audioTag = f[0];\u000a		var tag = macros['audioLoop'].audioLoops[audioTag];\u000a		if (tag == null) {\u000a			console.log("Tried to play audio stream tagged \u005c'" + audioTag + "\u005c' without loading it.");\u000a			return;\u000a		}\u000a\u000a		// Can we just play?\u000a		if (tag.isReady) {\u000a			tag.playLoop();\u000a		} else {\u000a			// The audio wasn't loaded yet - we could do this a better way, but just to avoid a race condition\u000a			// we're just going to poll\u000a			macros['startMusicLoop'].pendingIntervals[audioTag] = setInterval(function() {\u000a				if (tag.isReady) {\u000a					tag.playLoop();\u000a					clearInterval(macros['startMusicLoop'].pendingIntervals[audioTag]);\u000a					macros['startMusicLoop'].pendingIntervals[audioTag] = null;\u000a				}\u000a			}, 10);\u000a		}\u000a	}\u000a}\u000amacros['stopAudioLoop'] = {\u000a	handler: function (g, e, f, b) {\u000a		var audioTag = f[0];\u000a		var tag = macros['audioLoop'].audioLoops[audioTag];\u000a		if (tag != null) {\u000a			tag.stopLoop();\u000a			tag.interval = null;\u000a		}\u000a	}\u000a}\u000amacros['teletype'] = {\u000a	handler: function (g, e, f, b) {\u000a		if (f[0] == null) {\u000a			return;\u000a		}\u000a		var delayLen = (f[1] == null) ? 100 : f[1];\u000a\u000a		// First we create a span to type into and add it to the document\u000a		var ttSpan = insertElement(null, "span", null, "teletype");\u000a		g.appendChild(ttSpan);\u000a\u000a		jQuery(function(){\u000a			jQuery(ttSpan).teletype({\u000a				text: f[0],\u000a				animDelay: delayLen\u000a			});\u000a		});\u000a\u000a	}\u000a}\u000amacros['imageLink'] = {\u000a	handler: function (g, e, f, b) {\u000a		var imageTag = insertElement(null, "img", f[1], "imageLink");\u000a		imageTag.onclick = function() {\u000a			state.display(imageTag.id, imageTag);\u000a		}\u000a		g.appendChild(imageTag);\u000a	}\u000a}\u000a
